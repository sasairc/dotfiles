<?xml version="1.0"?>
<!DOCTYPE fontconfig SYSTEM "fonts.dtd">
<fontconfig>
	<dir>/usr/share/fonts</dir>
	<dir>~/.fonts</dir>
	<cachedir>~/.fontconfig/fbterm</cachedir>
	<match  target="font">
		<edit name="embeddedbitmap" mode="assign">
			<bool>false</bool>
		</edit>
		<edit name="antialias" mode="assign">
			<bool>true</bool>
		</edit>
	</match>
</fontconfig>
