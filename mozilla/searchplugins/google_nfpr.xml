<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
	<ShortName>Google en_US</ShortName>
	<Description>Google Search en_US</Description>
	<InputEncoding>UTF-8</InputEncoding>	<!-- フォームへ入力される文字コード -->
	<Image width="16" height="16">data:image/png;base64,</Image>	<!-- 検索エンジンのファビコン -->
	<Url type="text/html" method="GET" template="https://www.google.com/search">
		<Param name="q" value="{searchTerms}"/>
		<Param name="hq" value="-matome.naver -2ch -2chann -futalog -soku"/>	<!-- 除外するワード（隠しワード） -->
		<Param name="hl" value="en"/>										<!-- 検索する言語 -->
		<Param name="oe" value="utf-8"/>									<!-- クエリの文字コード -->
		<Param name="ie" value="utf-8"/>									<!-- 検索結果の文字コード -->
		<Param name="aq" value="t"/>										<!-- URLフォームからの検索であることを明示 -->
		<Param name="safe" value="off"/>									<!-- セーフサーチoff -->
		<Param name="num" value="20"/>										<!-- ページ毎に表示される検索結果の数 -->
		<Param name="nfpr" value="1" />										<!-- 「次の検索結果を表示しています」を無効 -->
	</Url>
	<SearchForm>https://www.google.com/</SearchForm>
</SearchPlugin>
